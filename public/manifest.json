{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "description": "Description of your extension",
    "permissions": ["tabs", "activeTab", "storage"],
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  